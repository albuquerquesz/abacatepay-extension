{
  "name": "abacatepay",
  "displayName": "AbacatePay",
  "description": "AbacatePay integration for Visual Studio Code",
  "version": "0.0.1",
  "publisher": "abacatepay",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/app.js",
  "contributes": {
    "commands": [
      {
        "command": "abacatepay.showPanel",
        "title": "AbacatePay: Open Panel"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "abacatepay",
          "title": "AbacatePay",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "abacatepay": []
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "compile": "tsc -p ./tsconfig.extension.json",
    "watch": "tsc -watch -p ./tsconfig.extension.json",
    "build": "npm run compile && npm run build:webview",
    "build:webview": "cd webview-ui && npm run build",
    "install:all": "npm install && cd webview-ui && npm install",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "tailwindcss": "^4.1.18",
    "zod": "^4.3.5"
  }
}